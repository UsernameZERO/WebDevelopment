<html>
  <head>
    <title><%= title%></title>
  </head>
  <body>
    <h1>Rendered from ejs -- > Contact lists</h1>

    <div>
      <ul>
        <% for(let i of contact_lists) { %>
        <li>
          <p><%= i.name %></p>
          <p><%= i.phone%></p>
        </li>
        <%}%>
      </ul>
    </div>

    <form action="/create_contacts" method="post">
      <input type="text" name="name" placeholder="Enter Name" required />
      <input type="number" name="phone" placeholder="Enter Phone" required />
      <button type="submit">Add Contact</button>
    </form>
  </body>
</html>
